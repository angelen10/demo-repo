<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="struts2" extends="struts-default">
		<action name="login" class="com.struts2.LoginAction" method="myExecute">
			<result name="success">/result.jsp</result>
		</action>
		<action name="userAction" class="com.struts2.UserAction">
			<result name="success">/output.jsp</result>
		</action>
		<action name="usersAction" class="com.struts2.UsersAction">
			<result name="success">/output.jsp</result>
		</action>
		<action name="registerAction" class="com.struts2.RegisterAction">
			<result name="success">/registerResult.jsp</result>
			<result name="input">/register.jsp</result>
		</action>
		
	</package>

</struts>